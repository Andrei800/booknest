# Настройки BookNest
# Скопируйте этот файл в .env и настройте под себя

# База данных (по умолчанию SQLite)
DATABASE_URL=sqlite:///./booknest.db

# Опционально: API ключ Google Books для более высоких лимитов
# GOOGLE_BOOKS_API_KEY=your_api_key_here

# Опционально: OpenAI для AI-функций (описания, рекомендации)
# OPENAI_API_KEY=your_openai_key_here

# Режим отладки
DEBUG=true
